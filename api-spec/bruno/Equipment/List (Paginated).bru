meta {
  name: List (Paginated)
  type: http
  seq: 1
}

get {
  url: {{base_url}}/api/equipment
  body: none
  auth: inherit
}

headers {
  Accept: application/json
}

params:query {
  page: {{equipment_page}}
  size: {{equipment_page_size}}
  sort: {{equipment_sort}}
}

vars:pre-request {
  equipment_page: 0
  equipment_page_size: 20
  equipment_sort: id,asc
}

settings {
  encodeUrl: true
  timeout: 0
}
