meta {
  name: Update (Patch)
  type: http
  seq: 5
}

patch {
  url: {{base_url}}/api/user/{{user_id}}
  body: raw
  auth: inherit
}

headers {
  Content-Type: application/json
  Accept: application/json
}

body:raw {
  {
    "lname": "{{user_patch_last_name}}",
    "email": "{{user_patch_email}}"
  }
}

vars:pre-request {
  user_id: 1
  user_patch_last_name: Lovelace-Smith
  user_patch_email: ada.lovelace-smith@example.com
}

settings {
  encodeUrl: true
  timeout: 0
}
